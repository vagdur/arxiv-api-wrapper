{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["./src/index.ts"],
  "out": "docs",
  "name": "arxiv-api-wrapper",
  "readme": "./README.md",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeInternal": true,
  "theme": "default",
  "sort": ["source-order"],
  "categorizeByGroup": true,
  "categoryOrder": [
    "Functions",
    "Interfaces",
    "Types"
  ],
  "gitRevision": "main",
  "gitRemote": "origin",
  "validation": {
    "invalidLink": true,
    "notDocumented": false
  }
}

